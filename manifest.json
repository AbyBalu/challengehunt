{
  "manifest_version": 2,

  "name": "Challenge Hunt",
  "description": "All programming challenges at one place",
  "version": "0.1.3",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
    "scripts": ["js/background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "icons": { 
    "16": "icon.png",
    "48": "icon48.png"
  },

  "permissions": [
    "http://challengehuntapp.appspot.com/*"
  ]
}
