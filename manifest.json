{
  "manifest_version": 2,

  "name": "Challenge Hunt",
  "description": "All programming challenges at one place",
  "version": "0.3.4",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",

  "browser_action": {
    "default_icon": "img/image_64.png",
    "default_popup": "index2.html"
  },
  "icons": { 
    "16": "img/image_16.png",
    "48": "img/image_48.png",
    "64": "img/image_64.png",
    "128": "img/image_128.png"
  },

  "permissions": [
    "http://testchallengehunt.appspot.com/*",
    "https://github.com/ChallengeHunt/challengehunt/*",
    "https://buttons.github.io/buttons.js"
  ]
}
